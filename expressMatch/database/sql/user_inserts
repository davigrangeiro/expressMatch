select u.id, u.nick, i.name, a.authority, d.acronym from user u inner join user_info i on u.id = i.id inner join authorities a on u.id = a.user_id inner join institution d on i.instituition_id = d.id;
select * from authorities;
select * from user_info;
select * from institution